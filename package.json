{
  "name": "@litert/redis",
  "version": "3.0.2",
  "description": "A redis protocol implement for Node.js.",
  "main": "./lib/index.js",
  "scripts": {
    "prepublishOnly": "npm run rebuild",
    "build": "tsc -v && tsc -p .",
    "build-watch": "tsc -v && tsc -w -p .",
