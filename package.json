  },
  "homepage": "https://github.com/litert/redis.js#readme",
  "types": "./lib/index.d.ts",
  "typings": "./lib/index.d.ts",
  "dependencies": {
    "@litert/observable": "^0.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
